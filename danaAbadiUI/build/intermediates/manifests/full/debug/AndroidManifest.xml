<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ui.tech.sahabatmakara.activity"
    android:versionCode="9"
    android:versionName="1.8" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:screenOrientation="portrait"
        android:theme="@style/AppTheme" >
        <activity
            android:name="ui.tech.sahabatmakara.activity.Activity_Splashscreen"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="ui.tech.sahabatmakara.activity.Activity_MenuSlider"
            android:screenOrientation="portrait"
            android:theme="@style/ToolBarStyle" />
        <activity
            android:name="ui.tech.sahabatmakara.activity.Activity_Content"
            android:immersive="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="ui.tech.sahabatmakara.activity.Activity_ViewImage"
            android:immersive="true"
            android:screenOrientation="portrait" />
        <activity
            android:name="ui.tech.sahabatmakara.activity.Activity_DetailEvent"
            android:screenOrientation="portrait"
            android:theme="@style/ToolBarStyle" />
        <activity
            android:name="ui.tech.sahabatmakara.activity.Activity_DetailNews"
            android:screenOrientation="portrait"
            android:theme="@style/ToolBarStyle" />
        <activity
            android:name="ui.tech.sahabatmakara.activity.Activity_DetailProgram"
            android:screenOrientation="portrait"
            android:theme="@style/ToolBarStyle" />
        <activity
            android:name="ui.tech.sahabatmakara.activity.Activity_Donate_Program"
            android:configChanges="keyboardHidden"
            android:screenOrientation="portrait"
            android:theme="@style/ToolBarStyle" />
        <activity
            android:name="ui.tech.sahabatmakara.activity.Activity_Setting_Account_View"
            android:theme="@style/ToolBarStyle"
            android:windowSoftInputMode="stateHidden" />
        <activity
            android:name="ui.tech.sahabatmakara.activity.Activity_Setting_Account_Edit"
            android:screenOrientation="portrait"
            android:theme="@style/ToolBarStyle" />
        <activity
            android:name="ui.tech.sahabatmakara.activity.Activity_Login"
            android:screenOrientation="portrait"
            android:theme="@style/ToolBarStyle" />
        <activity
            android:name="ui.tech.sahabatmakara.activity.Activity_Signup"
            android:screenOrientation="portrait"
            android:theme="@style/ToolBarStyle" />
        <activity
            android:name="ui.tech.sahabatmakara.activity.Activity_Donate_Donasi"
            android:screenOrientation="portrait"
            android:theme="@style/ToolBarStyle" />
        <activity
            android:name="ui.tech.sahabatmakara.activity.Activity_Selesai_Transfer"
            android:screenOrientation="portrait"
            android:theme="@style/ToolBarStyle" />
        <activity
            android:name="ui.tech.sahabatmakara.activity.Activity_Selesai_KartuKredit"
            android:screenOrientation="portrait"
            android:theme="@style/ToolBarStyle" />
        <activity
            android:name="ui.tech.sahabatmakara.activity.Activity_Selesai_JemputDana"
            android:screenOrientation="portrait"
            android:theme="@style/ToolBarStyle" />
        <activity
            android:name="ui.tech.sahabatmakara.activity.Activity_Selesai_UploadFoto"
            android:screenOrientation="portrait"
            android:theme="@style/ToolBarStyle" />
        <activity
            android:name="ui.tech.sahabatmakara.activity.Activity_TentangKami"
            android:screenOrientation="portrait"
            android:theme="@style/ToolBarStyle" />
        <activity
            android:name="ui.tech.sahabatmakara.activity.Activity_Webview"
            android:screenOrientation="portrait"
            android:theme="@style/ToolBarStyle" />
        <activity
            android:name="ui.tech.sahabatmakara.activity.UploadImageDialog"
            android:screenOrientation="portrait"
            android:theme="@style/DialogTheme" />

        <meta-data
            android:name="com.facebook.sdk.ApplicationId"
            android:value="@string/app_id_new" />

        <activity
            android:name="com.facebook.FacebookActivity"
            android:configChanges="keyboard|keyboardHidden|screenLayout|screenSize|orientation"
            android:label="@string/app_name"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />
    </application>

</manifest>